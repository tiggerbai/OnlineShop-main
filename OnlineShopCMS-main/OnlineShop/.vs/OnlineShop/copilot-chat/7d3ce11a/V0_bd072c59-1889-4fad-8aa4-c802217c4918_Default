</div>

@section Scripts {
    <script>
        $(document).ready(function () {
            $.ajax({
                url: '@Url.Action("Query", "Borad")',
                type: 'GET',
                success: function (data) {
                    var announcements = data.Grid;
                    var html = '';
                    for (var i = 0; i < announcements.length; i++) {
                        var announcement = announcements[i];
                        html += '<div class="announcement">';
                        html += '<h3>' + announcement.AnnoSubject + '</h3>';
                        html += '<p>' + announcement.AnnoContent + '</p>';
                        html += '<p><small>' + announcement.AnnoDate + '</small></p>';
                        html += '</div>';
                    }
                    $('#announcements').html(html);
                }
            });
        });
    </script>
}
