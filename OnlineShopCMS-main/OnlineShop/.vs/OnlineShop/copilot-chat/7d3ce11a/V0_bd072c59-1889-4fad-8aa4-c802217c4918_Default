@section Scripts {
    <script src="https://cdn.jsdelivr.net/npm/lottiefiles/lottie-player"></script>
    <script>
        var lottiePlayer = document.getElementById('lottie-animation');
        lottiePlayer.setAttribute('src', '/lottie/medical.json');
    </script>
    <script>
        var animation = lottie.loadAnimation({
            container: document.getElementById('lottie-animation'), // the dom element that will contain the animation
            renderer: 'svg',
            loop: true,
            autoplay: true,
            path: 'path_to_your_animation.json' // the path to the animation json
        });
    </script>
}
